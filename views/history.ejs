<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>History</title>
</head>
<body>
  <h1>Image History</h1>
  <ul>
    <% imageHistory.forEach(image => { %>
      <li><img src="<%= image %>" alt="Dog Image"></li>
    <% }); %>
  </ul>
</body>
</html>
